{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#app": ["node_modules/nuxt/dist/app"]
    },
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": [".nuxt/nuxt.d.ts", "**/*", "types/**/*"]
}
